{
  "name": "task-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --config nodemon.json index.js",
    "icons": "cd task_app && dart run flutter_launcher_icons",
    "splash": "cd task_app && dart run flutter_native_splash:create",
    "flutter-run": "cd task_app && flutter run",
